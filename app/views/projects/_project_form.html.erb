<%= form_for(project) do |f| %>

  <%= f.label :project_photo %>
  <%= f.file_field :project_photo %>
  <br> 
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <br> 
  <%= f.label :category_id %>
  <%= f.select :category_id, [["Art", 0], ["Crafts", 1], ["Dance", 2], ["Design", 3], ["Fashion", 4], ["Film", 5], ["Food", 6], ["Games", 7], ["Journalism", 8], ["Music", 9], ["Photography", 10], ["Publishing", 11], ["Technology", 12], ["Theater", 13]] %>

  <br>
  <%= f.label :target_amount %>
  <%= f.number_field :target_amount%>
  <br>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.submit %>
<% end %>  

